<?xml version="1.0"?>
<project name="Lab6MainFrame" default="remove-signatures" basedir=".">
    <target name="remove-signatures">
        <exec executable="zip">
            <arg value="-d"/>
            <arg value="out/artifacts/Lab6MainFrame_jar/Lab6MainFrame.jar"/>
            <arg value="*.RSA"/>
            <arg value="*.SF"/>
            <arg value="*.DSA"/>
        </exec>
    </target>
</project>



